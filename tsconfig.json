{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es5",
      "dom",
      "dom.iterable"
    ],
    "downlevelIteration": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "outDir": "dist",
    "baseUrl": "."
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
